{
    "name": "server",
    "engines": {
        "node": ">=9.0.0"
    },
    "scripts": {
        "this:install": "npm ci",
        "this:start": "nodemon --ignore **/*.test.* --exec ts-node -r tsconfig-paths/register ./src/server.ts",
        "this:build": "ts-node -r tsconfig-paths/register ./src/server.ts",
        "this:lint": "eslint .",
        "this:lintFix": "eslint --fix .",
        "this:runTest": "vitest run",
        "this:watchTest": "vitest --ui",
        "try": "ts-node-esm -r tsconfig-paths/register ./src/server.ts",
        "start": "ts-node-dev --respawn -r tsconfig-paths/register ./src/server.ts",
        "start2": "nodemon --ignore **/*.test.* --exec ts-node-esm -r tsconfig-paths/register ./src/server.ts",
        "//": ""
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.14.3",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^6.9.0",
        "ms": "^2.1.3",
        "nanoid": "^3.3.4",
        "nodemailer": "^6.9.1",
        "peer": "^0.6.1",
        "socket.io": "^4.5.4",
        "ts-node-dev": "^2.0.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/base64-stream": "^1.0.2",
        "@types/bcrypt": "^5.0.0",
        "@types/config": "^3.3.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^18.11.18",
        "@types/nodemailer": "^6.4.7",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.0",
        "@vitest/coverage-c8": "^0.28.4",
        "@vitest/ui": "^0.28.4",
        "eslint-plugin-node": "^11.1.0",
        "nodemon": "^2.0.20",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "vitest": "^0.28.4"
    }
}
