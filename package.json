{
    "name": "template",
    "version": "1.0.0",
    "description": "Template by LesnoyPudge",
    "keywords": [],
    "author": "LesnoyPudge",
    "license": "ISC",
    "scripts": {
        "client:install": "npm run this:install --prefix client",
        "client:uninstall": "rimraf client/node_modules",
        "client:start": "npm run this:start --prefix client",
        "client:build": "npm run this:build --prefix client",
        "client:lint": "npm run this:lint --prefix client",
        "client:lintFix": "npm run this:lintFix --prefix client",
        "client:generateImageTypes": "npm run this:generateImageTypes --prefix client",
        "client:optimizeImages": "npm run this:optimizeImages --prefix client",
        "//1": "",
        "server:install": "npm run this:install --prefix server",
        "server:uninstall": "rimraf server/node_modules",
        "server:start": "npm run this:start --prefix server",
        "server:build": "npm run this:build --prefix server",
        "server:lint": "npm run this:lint --prefix server",
        "server:lintFix": "npm run this:lintFix --prefix server",
        "server:runTest": "npm run this:runTest --prefix server",
        "server:watchTest": "npm run this:watchTest --prefix server",
        "//2": "",
        "project:install": "npm run prepare && npm run client:install && npm run server:install",
        "project:uninstall": "npm run client:uninstall && npm run server:uninstall",
        "project:reinstall": "npm run project:uninstall && npm run project:install",
        "project:start": "concurrently --kill-others \"npm run client:start\" \"npm run server:start\"",
        "project:build": "npm run client:build && npm run server:build",
        "project:lint": "eslint .",
        "project:lintFix": "eslint . --fix",
        "project:runTest": "npm run server:runTest",
        "//3": "",
        "prepare": "husky install",
        "deploy": "git add . && git commit -m 'update' && git push",
        "dev": "cross-env NODE_ENV=development CUSTOM_NODE_ENV=development npm run project:start",
        "prod": "cross-env NODE_ENV=production CUSTOM_NODE_ENV=production npm run project:build",
        "test": "npm run project:runTest",
        "//": ""
    },
    "dependencies": {
        "@reach/observe-rect": "^1.2.0",
        "@types/is-callable": "^1.1.0",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "is-callable": "^1.2.7",
        "is-odd": "^3.0.1",
        "json-schema-to-typescript": "^13.0.2"
    },
    "devDependencies": {
        "@types/is-odd": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "eslint": "^8.34.0",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.3",
        "rimraf": "^4.1.2",
        "svgo": "^3.0.2",
        "ts-essentials": "^9.3.2",
        "typescript": "^4.9.5"
    }
}