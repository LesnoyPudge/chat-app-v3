{
    "name": "template",
    "version": "1.0.0",
    "description": "Template by LesnoyPudge",
    "keywords": [],
    "author": "LesnoyPudge",
    "license": "ISC",
    "scripts": {
        "client:install": "npm run this:install --prefix client",
        "client:uninstall": "rimraf client/node_modules",
        "client:start": "cross-env NODE_ENV=development CUSTOM_NODE_ENV=development npm run this:start --prefix client",
        "client:build": "cross-env NODE_ENV=production CUSTOM_NODE_ENV=production npm run this:build --prefix client",
        "client:preview": "cross-env NODE_ENV=production CUSTOM_NODE_ENV=production npm run this:preview --prefix client",
        "client:productionDev": "cross-env NODE_ENV=production CUSTOM_NODE_ENV=production npm run this:start --prefix client",
        "client:lint": "npm run this:lint --prefix client",
        "client:lintFix": "npm run this:lintFix --prefix client",
        "client:generateImageStuff": "npm run this:generateImageStuff --prefix client",
        "//1": "",
        "server:install": "npm run this:install --prefix server",
        "server:uninstall": "rimraf server/node_modules",
        "server:start": "cross-env NODE_ENV=development CUSTOM_NODE_ENV=development npm run this:start --prefix server2",
        "server:build": "cross-env NODE_ENV=production CUSTOM_NODE_ENV=production npm run this:build --prefix server2",
        "server:lint": "npm run this:lint --prefix server",
        "server:lintFix": "npm run this:lintFix --prefix server",
        "server:runTest": "npm run this:runTest --prefix server",
        "server:watchTest": "npm run this:watchTest --prefix server",
        "//2": "",
        "project:install": "npm run prepare && npm run client:install && npm run server:install",
        "project:uninstall": "npm run client:uninstall && npm run server:uninstall",
        "project:reinstall": "npm run project:uninstall && npm run project:install",
        "project:start": "concurrently --kill-others \"npm run client:start\" \"npm run server:start\"",
        "project:build": "npm run client:build && npm run server:build",
        "project:lint": "eslint .",
        "project:lintFix": "eslint . --fix",
        "project:runTest": "npm run server:runTest",
        "//3": "",
        "prepare": "husky install",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "test": "npm run project:runTest",
        "gulp:get:version": "gulp --version",
        "gulp:get:tasks": "gulp --tasks",
        "gulp:start:client:dev": "gulp client_dev",
        "gulp:start:client:build": "gulp client_build",
        "parse": "i18next 'client/**/*.{ts,tsx}'",
        "//": ""
    },
    "dependencies": {
        "@reach/observe-rect": "^1.2.0",
        "@types/is-callable": "^1.1.0",
        "@types/js-cookie": "^3.0.3",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "is-callable": "^1.2.7",
        "is-hotkey": "^0.2.0",
        "is-odd": "^3.0.1",
        "js-cookie": "^3.0.5",
        "json-schema-to-typescript": "^13.0.2",
        "react-json-pretty": "^2.2.0",
        "workbox-background-sync": "^7.0.0",
        "workbox-broadcast-update": "^7.0.0",
        "workbox-cacheable-response": "^7.0.0",
        "workbox-core": "^7.0.0",
        "workbox-expiration": "^7.0.0",
        "workbox-google-analytics": "^7.0.0",
        "workbox-navigation-preload": "^7.0.0",
        "workbox-precaching": "^7.0.0",
        "workbox-range-requests": "^7.0.0",
        "workbox-routing": "^7.0.0",
        "workbox-strategies": "^7.0.0",
        "workbox-streams": "^7.0.0"
    },
    "devDependencies": {
        "@total-typescript/ts-reset": "^0.4.2",
        "@types/gulp": "^4.0.14",
        "@types/gulp-watch": "^4.1.36",
        "@types/is-odd": "^3.0.1",
        "@types/node": "^20.8.2",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "app-root-path": "^3.1.0",
        "eslint": "^8.34.0",
        "eslint-plugin-import": "^2.27.5",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "gulp-watch": "^5.0.1",
        "husky": "^8.0.3",
        "i18next-parser": "^8.8.0",
        "i18next-scanner": "^4.4.0",
        "rimraf": "^4.1.2",
        "svgo": "^3.0.2",
        "ts-essentials": "^9.3.2",
        "ts-node": "^10.9.1",
        "types-spring": "^2.0.0",
        "typescript": "^5.3.2"
    }
}
